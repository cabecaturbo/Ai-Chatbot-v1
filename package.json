{
  "name": "netia-chatbot-server",
  "version": "1.0.0",
  "description": "A modern chatbot server with WebSocket support",
  "main": "src/server.js",
  "scripts": {
    "start": "node src/server.js",
    "dev": "nodemon src/server.js",
    "test": "jest",
    "safe:dev": "cd Ai-Chatbot-v1/netia-bot-safe && npm run dev",
    "safe:check:health": "cd Ai-Chatbot-v1/netia-bot-safe && npm run check:health",
    "safe:check:webhook": "cd Ai-Chatbot-v1/netia-bot-safe && npm run check:webhook",
    "safe:test:conversations": "cd Ai-Chatbot-v1/netia-bot-safe && npm run test:conversations",
    "safe:test:load": "cd Ai-Chatbot-v1/netia-bot-safe && npm run test:load"
  },
  "keywords": ["chatbot", "websocket", "nodejs", "express"],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "socket.io": "^4.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1"
  },
  "devDependencies": {
    "nodemon": "^3.0.2"
  }
}
